<% modal id: "ChangeModel-#{object.id}-ChangeActionOrDescription-modal" do %>
  <% card_header do %>
    ChangeHeader
  <% end %> 
  <% card_body do %> 
    <p>ChangeInfo.</p>
    <div class="pt-2 flex justify-end">
      <%= content_tag('A', 'OK', class: 'btn btn', data: { action: 'click->modal#hide' }) %>
    </div>
  <% end %> 
<% end %> 
